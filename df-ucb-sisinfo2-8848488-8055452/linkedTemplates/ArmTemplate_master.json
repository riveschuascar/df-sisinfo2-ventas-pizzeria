{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-ucb-sisinfo2-8848488-8055452"
		},
		"ls_sa_ventas_csv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa_ventas_csv'"
		},
		"ls_sa_ventas_xlsx_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sa_ventas_xlsx'"
		},
		"ls_sql_dw_ventas_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_dw_ventas'"
		},
		"ls_sql_dw_ventas_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-ucb-sisinfo2-8848488-8055452.database.windows.net"
		},
		"ls_sql_dw_ventas_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw-ventas"
		},
		"ls_sql_dw_ventas_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-ucb-sisinfo2-8848488-8055452_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sa_ventas_csv_connectionString": {
						"value": "[parameters('ls_sa_ventas_csv_connectionString')]"
					},
					"ls_sa_ventas_xlsx_connectionString": {
						"value": "[parameters('ls_sa_ventas_xlsx_connectionString')]"
					},
					"ls_sql_dw_ventas_password": {
						"value": "[parameters('ls_sql_dw_ventas_password')]"
					},
					"ls_sql_dw_ventas_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_dw_ventas_properties_typeProperties_server')]"
					},
					"ls_sql_dw_ventas_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_dw_ventas_properties_typeProperties_database')]"
					},
					"ls_sql_dw_ventas_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_dw_ventas_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}